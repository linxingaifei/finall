{
  "log": {
    "level": "info"
  },
  "inbounds": [
    {
      "type": "tun",
      "tag": "sb-tun",
      "interface_name": "sb-tun0",
      "mtu": 1500,
      "auto_route": false,
      "stack": "system"
    }
  ],
  "outbounds": [
    {
      "type": "selector",
      "tag": "proxy",
      "outbounds": [
        "node_a",
        "node_b"
      ]
    },
    {
      "type": "socks",
      "tag": "node_a",
      "server": "1.2.3.4",
      "server_port": 1080
    },
    {
      "type": "vless",
      "tag": "node_b",
      "server": "5.6.7.8",
      "server_port": 443,
      "uuid": "UUID-HERE",
      "flow": "xtls-rprx-vision",
      "tls": {
        "enabled": true
      }
    },
    {
      "type": "direct",
      "tag": "direct"
    }
  ],
  "route": {
    "auto_detect_interface": true,
    "rules": [
      {
        "geoip": ["cn"],
        "outbound": "direct"
      },
      {
        "outbound": "proxy"
      }
    ]
  }
}
